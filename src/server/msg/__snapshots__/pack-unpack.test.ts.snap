    "refCond": 48,
    "refNoun": 48,
    "refVerb": 32,
    "sequence": 1,
    "talker": 3,
    "text": "\\"We live far to the south in the desert lands of Shapeir. Someday we may yet return there.\\" ",
    "verb": 5,
